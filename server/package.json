{
  "name": "youmall",
  "version": "1.0.0",
  "description": "Projeto de ecommerce para ongs e instituiçoes beneficentes possuirem uma forma de divulgar melhor seus bazares",
  "main": "server.js",
  "authors": [
    "Ariel Sales de Oliveira Sobreira",
    "Matheus Maximiano de Melo Vieira"
  ],
  "repository": "https://github.com/youmallservice/app",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "doc": "apidoc -i src/app/controllers -o doc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "tsc && node build/server.js",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js"
  },
  "apidoc": {
    "title": "Documentação do Server Youmall",
    "url": "http://localhost:3333"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.1",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "apidoc": "^0.29.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.0",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  }
}
